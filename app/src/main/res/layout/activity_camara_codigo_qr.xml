<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/camara_c_digo_qr"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/content_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <RelativeLayout
                android:id="@+id/page_camara_c_digo_qr_ek1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp">

                <View
                    android:id="@+id/_bg__camara_c_digo_qr_ek2"
                    android:layout_width="match_parent"
                    android:layout_height="800dp"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
                    android:background="@drawable/_bg__camara_c_digo_qr_ek2_shape" />


                <View
                    android:id="@+id/rectangle_15_ek2"
                    android:layout_width="match_parent"
                    android:layout_height="600dp"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="66dp"
                    android:background="@drawable/rectangle_15_ek2_shape" />

                <RelativeLayout
                    android:id="@+id/group_57"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="-16dp"
                    android:layout_marginTop="0dp">

                    <ImageView
                        android:id="@+id/rectangle_21_ek2"
                        android:layout_width="match_parent"
                        android:layout_height="66dp"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="0dp"
                        android:contentDescription="rectangle_21_ek2"
                        android:src="@drawable/rectangle_21_ek2" />

                    <TextView
                        android:id="@+id/lectura_qr"
                        android:layout_width="185dp"
                        android:layout_height="54dp"
                        android:layout_marginLeft="120dp"
                        android:layout_marginTop="16dp"
                        android:text="@string/lectura_qr_string"
                        android:textColor="@color/registro_color"
                        android:textSize="28sp"
                        android:textStyle="bold" />

                </RelativeLayout>

                <ImageView
                    android:id="@+id/_atras__2__3"
                    android:layout_width="49dp"
                    android:layout_height="47dp"
                    android:layout_marginStart="13dp"
                    android:layout_marginTop="12dp"
                    android:contentDescription="_atras__2__3"
                    android:src="@drawable/_atras__2__3" />

            </RelativeLayout>


            <ImageView
                android:id="@+id/_inicio_ek8"
                android:layout_width="360dp"
                android:layout_height="800dp"
                android:layout_marginStart="-863dp"
                android:layout_marginTop="-1148dp"
                android:contentDescription="_inicio_ek8"
                android:src="@drawable/_inicio_ek8" />


            <ImageView
                android:id="@+id/busqueda_creacion"
                android:layout_width="360dp"
                android:layout_height="800dp"
                android:layout_marginStart="-830dp"
                android:layout_marginTop="651dp"
                android:contentDescription="busqueda_creacion"
                android:src="@drawable/busqueda_creacion" />

        </RelativeLayout>

    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>