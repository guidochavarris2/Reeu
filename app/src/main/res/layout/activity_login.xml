<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_container"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">


    <ImageView
        android:id="@+id/_inicio_ek8"
        android:layout_width="413dp"
        android:layout_height="800dp"
        android:contentDescription="_inicio_ek8"
        android:src="@drawable/_inicio_ek8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/page_login_ek1" />


    <RelativeLayout
        android:id="@+id/page_login_ek1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <View
            android:id="@+id/_bg__login_ek2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:background="@drawable/_bg__login_ek2_shape" />

        <TextView
            android:id="@+id/inicio_de_sesi_n"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/__deja_que_tu_smartphone_sea_el_gerente_de_tu_evento_"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="20dp"
            android:text="@string/inicio_de_sesi_n_string"
            android:textColor="@color/inicio_de_sesi_n_color"
            android:textSize="40sp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/iniciar_con_tu_cuenta_de_reeu_"
            android:layout_width="233dp"
            android:layout_height="19dp"
            android:layout_below="@+id/inicio_de_sesi_n"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="36dp"
            android:layout_marginTop="15dp"
            android:contentDescription="iniciar_con_tu_cuenta_de_reeu_"
            android:src="@drawable/iniciar_con_tu_cuenta_de_reeu_" />

        <ImageView
            android:id="@+id/dni_ek1"
            android:layout_width="32dp"
            android:layout_height="22dp"
            android:layout_below="@+id/iniciar_con_tu_cuenta_de_reeu_"
            android:layout_alignParentStart="true"
            android:layout_marginStart="36dp"
            android:layout_marginTop="15dp"
            android:contentDescription="dni_ek1"
            android:src="@drawable/dni_ek1" />

        <ImageView
            android:id="@+id/password_ek1"
            android:layout_width="86dp"
            android:layout_height="22dp"
            android:layout_below="@+id/dni_ek1"
            android:layout_alignParentStart="true"

            android:layout_marginStart="36dp"
            android:layout_marginTop="85dp"
            android:contentDescription="password_ek1"
            android:src="@drawable/password_ek1" />

        <RelativeLayout
            android:id="@+id/group_41"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/group_46"
            android:layout_marginStart="36dp"
            android:layout_marginTop="30dp"
            tools:layout_centerHorizontal="true">

            <Button
                android:id="@+id/btnLogin"
                android:layout_width="290dp"
                android:layout_height="50dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:text="@string/inicia_sesi_n_ek1_string"
                android:textColor="@color/registro_color"
                android:textSize="18sp"
                android:textStyle="bold"
                app:backgroundTint="@null"
                android:contentDescription="_rectangle_5_ek1"
                android:background="@drawable/_rectangle_5_ek1" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/group_46"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/password_ek1"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="36dp"
            android:layout_marginTop="20dp">

            <View
                android:id="@+id/rectangle_3_ek1"
                android:layout_width="290dp"
                android:layout_height="50dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:alpha="0.10000000149012"
                android:background="@drawable/rectangle_2_ek1_shape" />

            <EditText
                android:id="@+id/etUserPassword"
                android:layout_width="276dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="15dp"
                android:ems="10"
                android:hint="@string/introduce_tu_contrase_a_ek1_string"
                android:inputType="text"
                android:textColor="@color/_99999999_color"
                android:textColorHint="@color/_99999999_color"
                android:textSize="18sp"
                tools:ignore="TouchTargetSizeCheck" />

        </RelativeLayout>

        <TextView
            android:id="@+id/tvRegister"
            android:layout_width="213dp"
            android:layout_height="27dp"
            android:layout_below="@+id/group_41"
            android:layout_alignParentBottom="true"
            android:layout_gravity="center_horizontal|center"
            android:layout_marginLeft="77dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="50dp"
            android:gravity="center"
            android:text="@string/___no_tienes_cuenta__registrate_string"
            android:textColor="@color/registro_color"
            android:textSize="14sp"
            android:textStyle="bold"
            tools:layout_centerHorizontal="true" />


        <RelativeLayout
            android:id="@+id/group_45"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/dni_ek1"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="36dp"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="15dp">

            <View
                android:id="@+id/rectangle_2_ek1"
                android:layout_width="290dp"
                android:layout_height="50dp"
                android:layout_alignParentTop="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:alpha="0.10000000149012"
                android:background="@drawable/rectangle_2_ek1_shape"
                tools:layout_centerHorizontal="true" />

            <EditText
                android:id="@+id/etUserName"
                android:layout_width="271dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="0dp"
                android:ems="10"
                android:hint="@string/_999999999_string"
                android:inputType="text"
                android:textColor="@color/_99999999_color"
                android:textColorHint="@color/_99999999_color"
                android:textSize="18sp"
                tools:ignore="TouchTargetSizeCheck"
                tools:layout_centerHorizontal="true" />

        </RelativeLayout>

        <ImageView
            android:id="@+id/reeu_1"
            android:layout_width="275dp"
            android:layout_height="136dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="68dp"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="68dp"
            android:contentDescription="reeu_1"
            android:src="@drawable/reeu_1"
            tools:layout_centerHorizontal="true" />

        <TextView
            android:id="@+id/__deja_que_tu_smartphone_sea_el_gerente_de_tu_evento_"
            android:layout_width="310dp"
            android:layout_height="72dp"
            android:layout_below="@+id/reeu_1"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="53dp"
            android:layout_marginTop="10dp"
            android:text="@string/__deja_que_tu_smartphone_sea_el_gerente_de_tu_evento__string"
            android:textColor="@color/registro_color"
            android:textSize="20sp"
            android:textStyle="bold" />

        <ProgressBar
            android:id="@+id/progressBar"
            android:visibility="gone"
            style="?android:attr/progressBarStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.456" />

    </RelativeLayout>




</androidx.constraintlayout.widget.ConstraintLayout>